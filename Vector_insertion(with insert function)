//vector insertion with insert function

#include<iostream.h>
#include<vector>
int main(int argc, char *argv[])
{
	vector<int>vect1(5);
	vector<int>vect2(5,100);
	 vect1.insert(vect1.begin()+1,3,40);//inserting 40 3 times at 1  st index of vect1
	 for(vector<int>::iterator it=vect1.begin();it!=vect1.end();it++)
	 {
	 cout<<*it<<" "<<&it<<endl;
	 }
	 vect2.insert(vect2.begin(),50);//inserting at 0 th index of vect2
	 for(int i:vect2)
	 {
	 	cout<<endl<<i;
	 }
}
